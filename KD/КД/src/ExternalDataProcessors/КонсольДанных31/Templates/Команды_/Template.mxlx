			<c>
				<c>
					<f>0</f>
					<tl>
						<v8:item>
							<v8:lang>ru</v8:lang>
							<v8:content>	Колво = ТабЗначений.Количество();
	КолвоН = Колво;
	Сообщить(Колво);
	КПопыток = 7;
	ВремяПроведения = 2;
	ВремяЦикла = 2.5;
	ВремяПроведенияПослеУскорения = 10;
	ВремяЦиклаПослеУскорения = 10.01;
	ДатаУскорения = '20160715184000';
	ДатаЗавершения = '20160718064000';
	Комментировать = Истина;
	ВестиЛог = Ложь;
	ВремяХ = ТекущаяДата();
	ВремяС = ВремяХ;
	Чемпион = "";
	ДедЛоки = 0;
	Рекорд = 0;
	ЧистоеВремя = 0;
	Лог = "" + Колво + Символы.ПС;
	Для Каждого СтрокаТП Из ТабЗначений Цикл
		Колво = Колво - 1;
		Пока ТекущаяДата() - ВремяХ &gt; ВремяПровеления * 60 Цикл
			Если ТекущаяДата() - ВремяХ &gt; ВремяЦикла * 60 Тогда
				ВремяХ = ТекущаяДата()
			КонецЕсли;
			ОбработкаПрерыванияПользователя()
		КонецЦикла;
		Куда = СтрокаТП.Регистратор.ПолучитьОбъект();
		Куда.ДополнительныеСвойства.Вставить("ОтключитьМеханизмРегистрацииОбъектов");
		Куда.ДополнительныеСвойства.Вставить("ИТ_ПроводитьБезПроверок", Истина);
		ЭтоДедЛок = Истина;
		Ё = 0;
		Пока ЭтоДедЛок Цикл
			ВремяН = ТекущаяДата();
			ЭтоДедЛок = Ложь;
			ТекТип = ТипЗнч(Куда);
			Попытка
				Куда.Записать(РежимЗаписиДокумента.Проведение);
				ВремяОк = ТекущаяДата();
				ТекВремя = ВремяОк - ВремяН;
				КВыводу = "" + ВремяОк + ", ост:" + Колво + ":_Записан: " + Куда;
				Если Комментировать Тогда
					Сообщить(КВыводу);
				КонецЕсли;
				Если ВестиЛог Тогда
					Лог = Лог + КВыводу + Символы.ПС;
				КонецЕсли; 
			Исключение
				ВремяОк = ТекущаяДата();
				ТекВремя = ВремяОк - ВремяН;
				ТекстОшибки = СокрЛП(ОписаниеОшибки());
				ЭтоДедЛок = Найти(ВРег(ТекстОшибки), "DEADLOCK") &gt; 0;
				Если ЭтоДедЛок Тогда
					Ё = Ё + 1;
					ДедЛоки = ДедЛоки + ТекВремя;
					Если Ё = КПопыток Тогда
						ЭтоДедЛок = Ложь;
					КонецЕсли
				КонецЕсли;
				Если НЕ ЭтоДедЛок Тогда
					КВыводу = "!!! " + ВремяОк + " НЕ_Записан: " + Куда + ":" + ОписаниеОшибки(), СтатусСообщения.Важное;
					Если Комментировать Тогда
						Сообщить(КВыводу);
					КонецЕсли;
					Если ВестиЛог Тогда
						Лог = Лог + КВыводу + Символы.ПС;
					КонецЕсли; 
				КонецЕсли
			КонецПопытки;
			ОбработкаПрерыванияПользователя()
		КонецЦикла;
		Если ВремяОк &gt; ДатаУскорения Тогда
			ВремяПровеления = ВремяПровеленияПослеУскорения;
			ВремяЦикла = ВремяЦиклаПослеУскорения;
		КонецЕсли;
		ЧистоеВремя = ЧистоеВремя + ТекВремя;
		Если ТекВремя &gt; Рекорд Тогда
			Чемпион = СтрокаТП.Регистратор;
			Рекорд = ТекВремя;
		КонецЕсли;
		Если ВремяОк &gt; ДатаЗавершения Тогда
			Прервать;
		КонецЕсли;
	КонецЦикла;

	КВыводу = "Обработано документов: " + (КолвоН - Колво) + " за " + (ВремяОк - ВремяС) + " сек., чистое = " + ЧистоеВремя + " сек.; в среднем на док.= " + ЧистоеВремя/(КолвоН - Колво) + " сек., самый медленный " + Чемпион + " проводился " + Рекорд + " сек. Не успел обработать док. = " + Колво + ", затрачено на дедлоки " + ДедЛоки + " сек.";
	Если Комментировать Тогда
		Сообщить(КВыводу);
	КонецЕсли;
	Если ВестиЛог Тогда
		Лог = Лог + КВыводу + Символы.ПС;
		Текст = Новый ТекстовыйДокумент;
		Текст.УстановитьТекст(Лог);
		Текст.Записать("ИмяФайла");
	КонецЕсли; 
</v8:content>
